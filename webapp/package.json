{
	"name": "webapp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "doppler run -- vite dev",
		"build": "doppler run -- vite build",
		"preview": "doppler run -- vite preview",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"clean": "rimraf .svelte-kit",
		"test": "vitest --coverage",
		"test-staging": "vitest --coverage --reporter=junit --outputFile=./reports/junit.xml",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"lighthouse": "doppler run --command='LIGHTHOUSE_ENABLED=true && vite preview'",
		"lighthouse-dev": "lhci autorun --config=./.lighthouserc-dev.cjs",
		"lighthouse-staging": "lhci autorun --config=./.lighthouserc.cjs",
		"deploy": "npx wrangler deploy -e production"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.9.4",
		"@dimforge/rapier3d-compat": "^0.17.3",
		"@melt-ui/pp": "^0.3.2",
		"@melt-ui/svelte": "^0.86.6",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/adapter-cloudflare": "^7.0.5",
		"@sveltejs/kit": "^2.23.0",
		"@tailwindcss/postcss": "^4.1.11",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.11",
		"@testing-library/svelte": "^5.2.8",
		"@vitest/coverage-v8": "^3.2.4",
		"@yushijinhun/three-minifier-rollup": "^0.4.0",
		"@zerodevx/svelte-img": "^2.1.2",
		"c8": "^10.1.3",
		"eslint": "^9.31.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.10.1",
		"highlight.js": "^11.11.1",
		"jsdom": "^26.1.0",
		"kolorist": "^1.8.0",
		"magic-string": "^0.30.17",
		"mdsvex": "^0.12.6",
		"mermaid": "^11.8.1",
		"miragejs": "^0.1.48",
		"npm": "^11.4.2",
		"playwright": "^1.54.1",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"rehype-mermaid": "^3.0.0",
		"remark-footnotes": "2.0",
		"remark-gfm": "^4.0.1",
		"resize-observer-polyfill": "^1.5.1",
		"sharp": "^0.34.3",
		"shiki": "^3.7.0",
		"svelte": "^5.35.6",
		"svelte-awesome-icons": "^2.0.1",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.8.3",
		"vite": "^6.3.5",
		"vite-imagetools": "^7.1.0",
		"vitest": "^3.0.5",
		"wrangler": "^4.24.0"
	},
	"type": "module",
	"dependencies": {
		"@threlte/core": "^8.1.3",
		"@threlte/extras": "^9.4.2",
		"@tsparticles/engine": "^3.8.1",
		"@tsparticles/interaction-external-trail": "3.8",
