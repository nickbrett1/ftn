{
	"name": "webapp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "doppler run --command='vite dev'",
		"build": "doppler run --command='vite build'",
		"preview": "doppler run --command='vite preview'",
		"lighthouse": "doppler run --command='LIGHTHOUSE_ENABLED=true && vite preview'",
		"test": "vitest --coverage",
		"test-staging": "vitest --coverage --reporter=junit --outputFile=./reports/junit.xml",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"lighthouse-dev": "lhci autorun --config=./.lighthouserc-dev.cjs",
		"lighthouse-staging": "lhci autorun --config=./.lighthouserc.cjs",
		"deploy": "npx wrangler pages deploy .svelte-kit/cloudflare --commit-dirty=true --project-name=ftn"
	},
	"devDependencies": {
		"@dimforge/rapier3d-compat": "^0.15.0",
		"@melt-ui/pp": "^0.3.2",
		"@melt-ui/svelte": "^0.86.5",
		"@sveltejs/adapter-auto": "^5.0.0",
		"@sveltejs/adapter-cloudflare": "^6.0.1",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/postcss": "^4.0.17",
		"@testing-library/svelte": "^5.2.7",
		"@vite-pwa/sveltekit": "^0.6.7",
		"@vitest/coverage-v8": "^3.0.9",
		"@yushijinhun/three-minifier-rollup": "^0.4.0",
		"@zerodevx/svelte-img": "^2.1.2",
		"c8": "^10.1.3",
		"eslint": "^9.23.0",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-svelte": "^3.3.3",
		"jsdom": "^26.0.0",
		"kolorist": "^1.8.0",
		"magic-string": "^0.30.17",
		"miragejs": "^0.1.48",
		"npm": "^11.2.0",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"resize-observer-polyfill": "^1.5.1",
		"sharp": "^0.33.5",
		"svelte": "^5.25.3",
		"svelte-awesome-icons": "^2.0.1",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.0.17",
		"typescript": "^5.8.2",
		"vite": "^6.2.3",
		"vite-imagetools": "^7.0.5",
		"vitest": "^3.0.5"
	},
	"type": "module",
	"dependencies": {
		"@threlte/core": "^8.0.1",
		"@threlte/extras": "^9.1.0",
		"@tsparticles/engine": "^3.8.1",
		"@tsparticles/interaction-external-trail": "3.8",
		"@tsparticles/shape-text": "^3.8.1",
		"@tsparticles/slim": "^3.8.1",
		"cookie": "^1.0.2",
		"fauna": "^2.5.0",
		"graphql": "^16.10.0",
		"gsap": "^3.12.7",
		"most-visible": "^2.0.0",
		"nanoid": "^5.1.5",
		"three": "^0.174.0",
		"tippy.js": "^6.3.7",
		"tsparticles": "^3.8.1"
	}
}
