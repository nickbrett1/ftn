{
	"name": "webapp",
	"version": "0.0.1",
	"private": true,
	"overrides": {
		"cookie": "^1.0.2"
	},
	"scripts": {
		"postinstall": "node scripts/copy-pdf-worker.js",
		"predev": "node scripts/copy-pdf-worker.js",
		"dev": "npm run predev && doppler run -- vite dev",
		"prebuild": "node scripts/copy-pdf-worker.js",
		"build": "npm run prebuild && doppler run -- vite build",
		"check-pdf-worker": "node scripts/check-pdf-worker.js",
		"preview": "doppler run -- vite preview",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"clean": "rimraf .svelte-kit",
		"test": "vitest --coverage",
		"test:once": "vitest run",
		"test-staging": "vitest --coverage --reporter=junit --outputFile=./reports/junit.xml",
		"test-ci": "vitest --coverage --run --no-watch",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"lighthouse": "doppler run --command='LIGHTHOUSE_ENABLED=true && vite preview'",
		"lighthouse-dev": "lhci autorun --config=./.lighthouserc-dev.cjs",
		"lighthouse-staging": "lhci autorun --config=./.lighthouserc.cjs",
		"deploy": "npx wrangler deploy -e production"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.11.4",
		"@dimforge/rapier3d-compat": "^0.18.2",
		"@melt-ui/pp": "^0.3.2",
		"@melt-ui/svelte": "^0.86.6",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-cloudflare": "^7.1.3",
		"@sveltejs/kit": "^2.29.1",
		"@tailwindcss/postcss": "^4.1.11",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.11",
		"@testing-library/svelte": "^5.2.8",
		"@vitest/coverage-v8": "^3.2.4",
		"@yushijinhun/three-minifier-rollup": "^0.4.0",
		"@zerodevx/svelte-img": "^2.1.2",
		"c8": "^10.1.3",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"eslint-plugin-security": "^3.0.1",
		"highlight.js": "^11.11.1",
		"jsdom": "^26.1.0",
		"kolorist": "^1.8.0",
		"magic-string": "^0.30.17",
		"mdsvex": "^0.12.6",
		"mermaid": "^11.9.0",
		"miragejs": "^0.1.48",
		"npm": "^11.5.2",
		"playwright": "^1.54.2",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"rehype-mermaid": "^3.0.0",
		"remark-footnotes": "2.0",
		"remark-gfm": "^4.0.1",
		"resize-observer-polyfill": "^1.5.1",
		"sharp": "^0.34.3",
		"shiki": "^3.9.2",
		"svelte": "^5.38.1",
		"svelte-awesome-icons": "^2.0.1",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.1.12",
		"typescript": "^5.9.2",
		"vite": "^7.1.2",
		"vite-imagetools": "^8.0.0",
		"vitest": "^3.2.4",
		"wrangler": "^4.29.1",
		"safe-regex2": "^5.0.0"
	},
	"type": "module",
	"dependencies": {
		"@threlte/core": "^8.1.3",
		"@threlte/extras": "^9.4.4",
		"@tsparticles/engine": "^3.8.1",
		"@tsparticles/interaction-external-trail": "3.9",
		"@tsparticles/shape-text": "^3.9.1",
		"@tsparticles/slim": "^3.8.1",
		"apexcharts": "^5.3.3",
		"cookie": "^1.0.2",
		"gsap": "^3.13.0",
		"llama-api-client": "^0.1.5",
		"most-visible": "^2.0.0",
		"nanoid": "^5.1.5",
		"pdfjs-dist": "^5.4.54",
		"linkify-it": "^5.0.0",
		"three": "^0.179.0",
		"tippy.js": "^6.3.7",
		"tsparticles": "^3.9.1"
	}
}
