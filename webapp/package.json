{
  "name": "main",
  "main": "pages/index.js",
  "version": "1.0.0",
  "description": "Front-end and cloudworkers for bemstudios.uk",
  "repository": {
    "type": "git",
    "url": "https://github.com/nickbrett1/bem",
    "directory": "webapp"
  },
  "scripts": {
    "analyze": "ANALYZE=true doppler run --command='next build'",
    "build": "next build",
    "dev": "concurrently \"npm run wrangler-dev\" \"doppler run --command='npx next dev'\"",
    "wrangler-dev": "doppler run --command='wrangler dev --config ./worker/wrangler.toml  --var GOOGLE_CLIENT_ID:$GOOGLE_CLIENT_ID --var SENTRY_BACKEND_DSN:$SENTRY_BACKEND_DSN --var GOOGLE_CLIENT_SECRET:$GOOGLE_CLIENT_SECRET --var SENTRY_RELEASE:$SENTRY_RELEASE --var SENTRY_ENVIRONMENT:$SENTRY_ENVIRONMENT --port 8787 --local --host localhost:3000 --persist ./worker/src/index.js'",
    "wrangler-staging": "wrangler dev --port 8787 --local --host localhost:3000 ./worker/src/index.js --var GOOGLE_CLIENT_ID:$GOOGLE_CLIENT_ID --var SENTRY_BACKEND_DSN:$SENTRY_BACKEND_DSN --var GOOGLE_CLIENT_SECRET:$GOOGLE_CLIENT_SECRET --var SENTRY_RELEASE:$SENTRY_RELEASE --var SENTRY_ENVIRONMENT:staging ",
    "export": "next export",
    "lint": "next lint",
    "start": "next start",
    "staging": "concurrently \"npm run wrangler-staging\" \"next start\"",
    "test-staging": "jest --coverage --passWithNoTests --runInBand --ci --reporters=jest-junit --reporters=default",
    "test-dev": "jest --watchAll --passWithNoTests --coverage",
    "lighthouse-staging": "lhci autorun --config=./.lighthouserc.js",
    "lighthouse-dev": "lhci autorun --config=./.lighthouserc-dev.js",
    "wrangler-sourcemaps": "npx wrangler publish ./worker/src/index.js --dry-run --outdir out/worker --route bemstudios.uk",
    "deploy": "concurrently \"npx wrangler pages publish out --commit-dirty=true --project-name=bem\" \"npx wrangler publish ./worker/src/index.js --config ./worker/wrangler.toml --var GOOGLE_CLIENT_ID:$GOOGLE_CLIENT_ID --var GOOGLE_CLIENT_SECRET:$GOOGLE_CLIENT_SECRET --var SENTRY_BACKEND_DSN:$SENTRY_BACKEND_DSN --var SENTRY_RELEASE:$SENTRY_RELEASE --var SENTRY_ENVIRONMENT:$SENTRY_ENVIRONMENT \"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.4",
    "@sentry/nextjs": "^7.30.0",
    "graphql": "^16.6.0",
    "graphql-tools": "^8.3.15",
    "graphql-yoga": "^3.3.0",
    "nanoid": "^4.0.0",
    "next": "^13.1.2",
    "next-pwa": "^5.6.0",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "toucan-js": "^3.1.0",
    "urql": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@emotion/eslint-plugin": "^11.10.0",
    "@next/bundle-analyzer": "^13.1.2",
    "@next/eslint-plugin-next": "^13.1.2",
    "@sentry/browser": "^7.30.0",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "babel-loader": "^8.3.0",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.31.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-ejs": "^0.0.2",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-webpack-plugin": "^3.2.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-junit": "^15.0.0",
    "miragejs": "^0.1.47",
    "next-router-mock": "^0.8.0",
    "sentry-testkit": "^5.0.5",
    "typescript": "^4.9.4",
    "wrangler": "^2.7.1"
  }
}
