{
	"name": "webapp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "doppler run --command='vite dev'",
		"build": "doppler run --command='vite build'",
		"preview": "doppler run --command='vite preview'",
		"lighthouse": "doppler run --command='LIGHTHOUSE_ENABLED=true && vite preview'",
		"test": "vitest --coverage",
		"test-staging": "vitest --coverage --reporter=junit --outputFile=./reports/junit.xml",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"lighthouse-dev": "lhci autorun --config=./.lighthouserc-dev.cjs",
		"lighthouse-staging": "lhci autorun --config=./.lighthouserc.cjs",
		"deploy": "npx wrangler pages deploy .svelte-kit/cloudflare --commit-dirty=true --project-name=ftn"
	},
	"devDependencies": {
		"@dimforge/rapier3d-compat": "^0.14.0",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/adapter-cloudflare": "^5.0.1",
		"@sveltejs/kit": "^2.16.1",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@testing-library/svelte": "^5.2.6",
		"@vite-pwa/sveltekit": "^0.6.7",
		"@vitest/coverage-c8": "^0.33.0",
		"@vitest/coverage-v8": "^0.34.6",
		"@yushijinhun/three-minifier-rollup": "^0.4.0",
		"@zerodevx/svelte-img": "^2.1.2",
		"autoprefixer": "^10.4.20",
		"c8": "^10.1.3",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"jest-environment-jsdom": "^29.7.0",
		"jsdom": "^26.0.0",
		"kolorist": "^1.8.0",
		"magic-string": "^0.30.17",
		"miragejs": "^0.1.48",
		"npm": "^11.0.0",
		"postcss": "^8.5.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"resize-observer-polyfill": "^1.5.1",
		"sharp": "^0.33.5",
		"svelte": "^4.2.19",
		"svelte-awesome-icons": "^1.2.1",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^3.4.17",
		"vite": "^5.4.11",
		"vite-imagetools": "^7.0.5",
		"vitest": "^0.34.6"
	},
	"type": "module",
	"dependencies": {
		"@threlte/core": "^7.3.1",
		"@threlte/extras": "^8.12.2",
		"@tsparticles/engine": "^3.6.0",
		"@tsparticles/interaction-external-trail": "3.6",
		"@tsparticles/shape-text": "^3.6.0",
		"@tsparticles/slim": "^3.8.1",
		"@tsparticles/svelte": "^3.1.1",
		"cookie": "^1.0.2",
		"fauna": "^2.4.1",
		"graphql": "^16.10.0",
		"gsap": "^3.12.7",
		"nanoid": "^5.0.9",
		"three": "^0.172.0",
		"tsparticles": "^3.7.1"
	}
}
