{
	"name": "webapp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "doppler run --command='vite dev'",
		"build": "doppler run --command='vite build'",
		"preview": "doppler run --command='vite preview'",
		"lighthouse": "doppler run --command='LIGHTHOUSE_ENABLED=true && vite preview'",
		"test": "vitest --coverage",
		"test-staging": "vitest --coverage --reporter=junit --outputFile=./reports/junit.xml",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"lighthouse-dev": "lhci autorun --config=./.lighthouserc-dev.cjs",
		"lighthouse-staging": "lhci autorun --config=./.lighthouserc.cjs",
		"deploy": "npx wrangler pages publish .svelte-kit/cloudflare --commit-dirty=true --project-name=bem"
	},
	"devDependencies": {
		"@graphql-mocks/network-nock": "^0.5.0",
		"@playwright/test": "^1.32.0",
		"@sentry/vite-plugin": "^0.4.0",
		"@storybook/addon-essentials": "^7.0.0-rc.7",
		"@storybook/addon-interactions": "^7.0.0-rc.7",
		"@storybook/addon-links": "^7.0.0-rc.7",
		"@storybook/blocks": "^7.0.0-rc.4",
		"@storybook/svelte": "^7.0.0-rc.6",
		"@storybook/sveltekit": "^7.0.0-rc.7",
		"@storybook/testing-library": "^0.0.14--canary.30.5f44d14.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-cloudflare": "^2.2.0",
		"@sveltejs/kit": "^1.14.0",
		"@testing-library/svelte": "^3.2.2",
		"@vite-pwa/sveltekit": "^0.2.0",
		"@vitest/coverage-c8": "^0.29.7",
		"@zerodevx/svelte-img": "^1.2.10",
		"autoprefixer": "^10.4.14",
		"c8": "^7.13.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-storybook": "^0.6.11",
		"eslint-plugin-svelte3": "^4.0.0",
		"jest-environment-jsdom": "^29.5.0",
		"jsdom": "^21.1.1",
		"miragejs": "^0.1.47",
		"postcss": "^8.4.21",
		"prettier": "^2.8.6",
		"prettier-plugin-svelte": "^2.10.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"sharp": "^0.31.3",
		"storybook": "^7.0.0-rc.7",
		"svelte": "^3.57.0",
		"tailwindcss": "^3.2.7",
		"vite": "^4.2.1",
		"vite-imagetools": "^4.0.18",
		"vitest": "^0.29.7"
	},
	"type": "module",
	"dependencies": {
		"@sentry/svelte": "^7.41.0",
		"@sentry/tracing": "^7.45.0",
		"graphql": "^16.6.0",
		"nanoid": "^4.0.1"
	}
}
