{
	"name": "webapp",
	"version": "0.0.1",
	"private": true,
	"overrides": {
		"cookie": "^1.0.2"
	},
	"scripts": {
		"postinstall": "node scripts/copy-pdf-worker.js",
		"predev": "node scripts/copy-pdf-worker.js && node scripts/generate-cloudflare-worker.js",
		"dev": "npm run predev && doppler run --project webapp --config dev -- vite dev",
		"prebuild": "node scripts/copy-pdf-worker.js",
		"build": "vite build",
		"check-pdf-worker": "node scripts/check-pdf-worker.js",
		"preview": "vite preview",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"clean": "rimraf .svelte-kit",
		"test": "vitest --coverage",
		"test:once": "vitest run",
		"test-staging": "vitest --coverage --reporter=junit --outputFile=./reports/junit.xml",
		"test-ci": "vitest --coverage --run --no-watch",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"lighthouse": "sh -c 'LIGHTHOUSE_ENABLED=true vite preview'",
		"lighthouse-server": "sh -c 'LIGHTHOUSE_ENABLED=true vite preview'",
		"lighthouse-dev": "lhci autorun --config=./.lighthouserc-dev.cjs",
		"lighthouse-staging": "lhci autorun --config=./.lighthouserc.cjs",
		"deploy": "npx wrangler deploy -e production",
		"deploy-preview": "npx wrangler deploy --env preview"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.25.5",
		"@dimforge/rapier3d-compat": "^0.19.3",
		"@melt-ui/pp": "^0.3.2",
		"@melt-ui/svelte": "^0.86.6",
		"@playwright/test": "^1.58.2",
		"@sveltejs/adapter-auto": "^7.0.1",
		"@sveltejs/adapter-cloudflare": "^7.2.7",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.53.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/postcss": "^4.2.1",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.2.1",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.3.1",
		"@vitest/coverage-v8": "^4.0.18",
		"@yushijinhun/three-minifier-rollup": "^0.4.0",
		"@zerodevx/svelte-img": "^2.1.2",
		"c8": "^11.0.0",
		"eslint": "^10.0.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-security": "^4.0.0",
		"eslint-plugin-sonarjs": "^4.0.0",
		"eslint-plugin-svelte": "^3.15.0",
		"eslint-plugin-unicorn": "^63.0.0",
		"highlight.js": "^11.11.1",
		"jsdom": "^28.0.0",
		"kolorist": "^1.8.0",
		"magic-string": "^0.30.21",
		"mdsvex": "^0.12.6",
		"mermaid": "11.12.3",
		"miragejs": "^0.1.48",
		"npm": "^11.11.0",
		"playwright": "^1.58.2",
		"postcss": "^8.5.6",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"rehype-mermaid": "^3.0.0",
		"remark-footnotes": "2.0",
		"remark-gfm": "^4.0.1",
		"resize-observer-polyfill": "^1.5.1",
		"safe-regex2": "^5.0.0",
		"sharp": "^0.34.5",
		"shiki": "^3.23.0",
		"svelte": "5.50.3",
		"svelte-awesome-icons": "^3.0.1",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.2.1",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.1",
		"vite": "^7.3.1",
		"vite-imagetools": "^9.0.3",
		"vitest": "^4.0.17",
		"wrangler": "^4.69.0"
	},
	"type": "module",
	"dependencies": {
		"@threlte/core": "^8.4.0",
		"@threlte/extras": "^9.8.1",
		"@tsparticles/engine": "^3.8.1",
		"@tsparticles/interaction-external-trail": "3.9",
		"@tsparticles/shape-text": "^3.9.1",
		"@tsparticles/slim": "^3.8.1",
		"apexcharts": "^5.6.0",
		"arctic": "^3.7.0",
		"cookie": "^1.0.2",
		"gsap": "^3.14.2",
		"handlebars": "^4.7.8",
		"hi-base32": "^0.5.1",
		"linkify-it": "^5.0.0",
		"llama-api-client": "^0.3.0",
		"most-visible": "^2.0.0",
		"nanoid": "^5.1.6",
		"pdfjs-dist": "^5.4.624",
		"three": "^0.183.1",
		"tippy.js": "^6.3.7",
		"tsparticles": "^3.9.1"
	}
}
