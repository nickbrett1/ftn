{
  "name": "main",
  "main": "app/pages.jsx",
  "version": "1.0.0",
  "description": "Front-end and cloudworkers for bemstudios.uk",
  "repository": {
    "type": "git",
    "url": "https://github.com/nickbrett1/bem",
    "directory": "webapp"
  },
  "scripts": {
    "analyze": "ANALYZE=true doppler run --command='next build'",
    "build": "next build",
    "dev": "concurrently \"npm run wrangler-dev\" \"doppler run --command='npx next dev'\" \"npm run storybook  \"  ",
    "wrangler-dev": "doppler run --command='wrangler dev --config ./worker/wrangler.toml  --var GOOGLE_CLIENT_ID:$GOOGLE_CLIENT_ID --var SENTRY_BACKEND_DSN:$SENTRY_BACKEND_DSN --var GOOGLE_CLIENT_SECRET:$GOOGLE_CLIENT_SECRET --var SENTRY_RELEASE:$SENTRY_RELEASE --var SENTRY_ENVIRONMENT:$SENTRY_ENVIRONMENT --port 8787 --local --host localhost:3000 --persist ./worker/src/index.js'",
    "wrangler-staging": "wrangler dev --port 8787 --local --host localhost:3000 ./worker/src/index.js --var GOOGLE_CLIENT_ID:$GOOGLE_CLIENT_ID --var SENTRY_BACKEND_DSN:$SENTRY_BACKEND_DSN --var GOOGLE_CLIENT_SECRET:$GOOGLE_CLIENT_SECRET --var SENTRY_RELEASE:$SENTRY_RELEASE --var SENTRY_ENVIRONMENT:staging ",
    "export": "next export",
    "start": "next start",
    "staging": "NODE_ENV=test doppler run --command='concurrently \"npm run wrangler-staging\" \"next start\"'",
    "test-staging": "doppler run --command='jest --coverage --passWithNoTests --runInBand --ci --reporters=jest-junit --reporters=default'",
    "test-dev": "doppler run --command='jest --watchAll --passWithNoTests --coverage'",
    "lighthouse-staging": "lhci autorun --config=./.lighthouserc.js",
    "lighthouse-dev": "lhci autorun --config=./.lighthouserc-dev.js",
    "wrangler-sourcemaps": "npx wrangler publish ./worker/src/index.js --dry-run --outdir out/worker --route bemstudios.uk",
    "deploy": "concurrently \"npx wrangler pages publish .vercel/output/static --commit-dirty=true --project-name=bem\" \"npx wrangler publish ./worker/src/index.js --config ./worker/wrangler.toml --var GOOGLE_CLIENT_ID:$GOOGLE_CLIENT_ID --var GOOGLE_CLIENT_SECRET:$GOOGLE_CLIENT_SECRET --var SENTRY_BACKEND_DSN:$SENTRY_BACKEND_DSN --var SENTRY_RELEASE:$SENTRY_RELEASE --var SENTRY_ENVIRONMENT:$SENTRY_ENVIRONMENT \"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "next lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/nextjs": "^7.36.0",
    "nanoid": "^4.0.1",
    "next": "13.1.0",
    "next-pwa": "^5.6.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "sharp": "^0.31.3",
    "toucan-js": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@cloudflare/next-on-pages": "^0.2.0",
    "@next/bundle-analyzer": "^13.1.6",
    "@next/eslint-plugin-next": "^13.1.2",
    "@sentry/browser": "^7.36.0",
    "@storybook/addon-actions": "^7.0.0-beta.44",
    "@storybook/addon-backgrounds": "^7.0.0-beta.43",
    "@storybook/addon-docs": "^7.0.0-beta.40",
    "@storybook/addon-essentials": "^7.0.0-beta.44",
    "@storybook/addon-interactions": "^7.0.0-beta.44",
    "@storybook/addon-links": "^7.0.0-beta.43",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/blocks": "^7.0.0-beta.40",
    "@storybook/builder-webpack5": "^7.0.0-beta.40",
    "@storybook/manager-webpack5": "^7.0.0-alpha.10",
    "@storybook/nextjs": "^7.0.0-beta.44",
    "@storybook/react": "^7.0.0-beta.40",
    "@storybook/testing-library": "^0.0.14--canary.30.5f44d14.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "babel-plugin-styled-components": "^2.0.7",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.1.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-ejs": "^0.0.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-storybook": "^0.6.10",
    "eslint-webpack-plugin": "^3.2.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.2",
    "jest-junit": "^15.0.0",
    "miragejs": "^0.1.47",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "sentry-testkit": "^5.0.5",
    "storybook": "^7.0.0-beta.44",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.5",
    "vercel": "^28.15.2",
    "wrangler": "^2.9.1"
  }
}
