{
	"name": "webapp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "doppler run --command='vite dev'",
		"build": "doppler run --command='vite build'",
		"preview": "doppler run --command='vite preview'",
		"lighthouse": "doppler run --command='LIGHTHOUSE_ENABLED=true && vite preview'",
		"test": "vitest --coverage",
		"test-staging": "vitest --coverage --reporter=junit --outputFile=./reports/junit.xml",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"lighthouse-dev": "lhci autorun --config=./.lighthouserc-dev.cjs",
		"lighthouse-staging": "lhci autorun --config=./.lighthouserc.cjs",
		"deploy": "npx wrangler pages deploy .svelte-kit/cloudflare --commit-dirty=true --project-name=ftn"
	},
	"devDependencies": {
		"@dimforge/rapier3d-compat": "^0.11.2",
		"@graphql-mocks/network-nock": "^0.5.0",
		"@playwright/test": "^1.41.0",
		"@sveltejs/adapter-auto": "^3.0.1",
		"@sveltejs/adapter-cloudflare": "^3.0.2",
		"@sveltejs/kit": "^2.4.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@testing-library/svelte": "^4.0.5",
		"@vite-pwa/sveltekit": "^0.3.0",
		"@vitest/coverage-c8": "^0.33.0",
		"@vitest/coverage-v8": "^0.34.6",
		"@yushijinhun/three-minifier-rollup": "^0.4.0",
		"@zerodevx/svelte-img": "^2.1.0",
		"autoprefixer": "^10.4.17",
		"c8": "^8.0.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-storybook": "^0.6.15",
		"eslint-plugin-svelte": "^2.35.1",
		"i": "^0.3.7",
		"jest-environment-jsdom": "^29.7.0",
		"jsdom": "^23.2.0",
		"kolorist": "^1.8.0",
		"magic-string": "^0.30.5",
		"miragejs": "^0.1.48",
		"npm": "^10.3.0",
		"postcss": "^8.4.33",
		"prettier": "^3.2.4",
		"prettier-plugin-svelte": "^3.1.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"resize-observer-polyfill": "^1.5.1",
		"sharp": "^0.33.2",
		"storybook": "^7.6.9",
		"svelte": "^4.2.9",
		"svelte-grid": "^5.1.2",
		"svelte-preprocess": "^5.1.3",
		"tailwindcss": "^3.4.1",
		"vite": "^5.0.11",
		"vite-imagetools": "^6.2.9",
		"vitest": "^0.34.6"
	},
	"type": "module",
	"dependencies": {
		"@threlte/core": "^6.0.2",
		"@threlte/extras": "^5.0.4",
		"@tsparticles/engine": "^3.0.3",
		"@tsparticles/shape-text": "^3.0.3",
		"@tsparticles/slim": "^3.1.0",
		"@tsparticles/svelte": "^3.0.0",
		"cookie": "^0.6.0",
		"graphql": "^16.8.1",
		"gsap": "^3.12.4",
		"nanoid": "^5.0.4",
		"three": "^0.160.0",
		"tsparticles": "^3.1.0"
	}
}
