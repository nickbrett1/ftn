{
  "name": "main",
  "main": "pages/index.js",
  "version": "1.0.0",
  "description": "Front-end for bemstudios.uk",
  "repository": {
    "type": "git",
    "url": "https://github.com/nickbrett1/bem",
    "directory": "webapp"
  },
  "size-limit": [
    {
      "path": "assets/webpack_bundles/main-*.js",
      "limit": "2.4 s"
    }
  ],
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build && npx webpack-cli --config ./webpack.workers.config.js",
    "dev": "wrangler pages dev --proxy 3000 --script-path './workers/_worker.js' -- npx next dev",
    "export": "next export && cp ./workers/_worker-bundle.js ./out/_worker.js",
    "lint": "next lint",
    "start": "next start",
    "test": "jest --watch --passWithNoTests",
    "lighthouse": "npm run build && npm run export && lhci collect --collect.settings.chromeFlags='--headless --no-sandbox' && lhci assert",
    "deploy": "npx wrangler pages publish out --project-name=bem"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.0",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.10.7",
    "@react-oauth/google": "^0.2.8",
    "@sentry/nextjs": "^7.14.0",
    "jwt-decode": "^3.1.2",
    "next": "^12.3.1",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "toucan-js": "^2.7.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.16.0",
    "@emotion/eslint-plugin": "^11.10.0",
    "@next/bundle-analyzer": "^12.3.1",
    "@next/eslint-plugin-next": "^12.3.1",
    "@sentry/cli": "^2.6.0",
    "@sentry/webpack-plugin": "^1.19.0",
    "@size-limit/preset-app": "^8.1.0",
    "@size-limit/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ejs": "^0.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "imagemin": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.1.1",
    "progress-webpack-plugin": "^1.0.16",
    "serve": "^14.0.1",
    "typescript": "^4.8.4",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-bundle-tracker": "^1.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "wrangler": "^2.1.9"
  }
}
