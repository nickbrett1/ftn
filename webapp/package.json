{
	"name": "webapp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "doppler run --command='vite dev'",
		"build": "doppler run --command='vite build'",
		"preview": "doppler run --command='vite preview'",
		"lighthouse": "doppler run --command='LIGHTHOUSE_ENABLED=true && vite preview'",
		"test": "vitest --coverage",
		"test-staging": "vitest --coverage --reporter=junit --outputFile=./reports/junit.xml",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"lighthouse-dev": "lhci autorun --config=./.lighthouserc-dev.cjs",
		"lighthouse-staging": "lhci autorun --config=./.lighthouserc.cjs",
		"deploy": "npx wrangler pages deploy .svelte-kit/cloudflare --commit-dirty=true --project-name=ftn"
	},
	"devDependencies": {
		"@dimforge/rapier3d-compat": "^0.11.2",
		"@graphql-mocks/network-nock": "^0.5.0",
		"@playwright/test": "^1.37.0",
		"@storybook/addon-essentials": "^7.3.0",
		"@storybook/addon-interactions": "^7.3.0",
		"@storybook/addon-links": "^7.3.0",
		"@storybook/blocks": "^7.2.3",
		"@storybook/svelte": "^7.2.3",
		"@storybook/sveltekit": "^7.3.0",
		"@storybook/testing-library": "^0.2.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-cloudflare": "^2.3.2",
		"@sveltejs/kit": "^1.22.6",
		"@testing-library/svelte": "^4.0.3",
		"@vite-pwa/sveltekit": "^0.2.5",
		"@vitest/coverage-c8": "^0.33.0",
		"@vitest/coverage-v8": "^0.34.1",
		"@yushijinhun/three-minifier-rollup": "^0.4.0",
		"@zerodevx/svelte-img": "^2.0.1",
		"autoprefixer": "^10.4.14",
		"c8": "^8.0.1",
		"eslint": "^8.47.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-storybook": "^0.6.13",
		"eslint-plugin-svelte3": "^4.0.0",
		"i": "^0.3.7",
		"jest-environment-jsdom": "^29.6.2",
		"jsdom": "^22.1.0",
		"kolorist": "^1.8.0",
		"magic-string": "^0.30.2",
		"miragejs": "^0.1.47",
		"npm": "^9.8.1",
		"postcss": "^8.4.28",
		"prettier": "^3.0.2",
		"prettier-plugin-svelte": "^3.0.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"resize-observer-polyfill": "^1.5.1",
		"sharp": "^0.32.5",
		"storybook": "^7.3.0",
		"svelte": "^3.59.2",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.3",
		"vite": "^4.4.9",
