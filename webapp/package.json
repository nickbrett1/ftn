{
  "name": "main",
  "main": "pages/index.js",
  "version": "1.0.0",
  "description": "Front-end and cloudworkers for bemstudios.uk",
  "repository": {
    "type": "git",
    "url": "https://github.com/nickbrett1/bem",
    "directory": "webapp"
  },
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "dev": "concurrently --kill-others \"npm run wrangler-dev\" \"npx next dev\"",
    "wrangler-dev": "wrangler dev --port 8787 --local --host localhost:3000 ./worker/src/index.js --define SENTRY_RELEASE:\\'\\' --define SENTRY_ENVIRONMENT:\\'development\\' ",
    "wrangler-staging": "wrangler dev --port 8787 --local --host localhost:3000 ./worker/src/index.js --define SENTRY_RELEASE:\\'$SENTRY_RELEASE\\' --define SENTRY_ENVIRONMENT:\\'staging\\' ",
    "export": "next export",
    "lint": "next lint",
    "start": "next start",
    "staging": "concurrently --kill-others \"npm run wrangler-staging\" \"next start\"",
    "test-staging": "jest --passWithNoTests --runInBand --ci --reporters=jest-junit --reporters=default",
    "test-dev": "jest --watchAll --passWithNoTests --coverage",
    "lighthouse-staging": "lhci autorun --config=./.lighthouserc.js",
    "lighthouse-dev": "lhci autorun --config=./.lighthouserc-dev.js",
    "wrangler-sourcemaps": "npx wrangler publish ./worker/src/index.js --dry-run --outdir out/worker --route bemstudios.uk --define SENTRY_RELEASE:\\'$SENTRY_RELEASE\\' --define SENTRY_ENVIRONMENT:\\'$SENTRY_ENVIRONMENT\\'",
    "deploy": "concurrently \"npx wrangler pages publish out --commit-dirty=true --project-name=bem\" \"npx wrangler publish ./worker/src/index.js --route bemstudios.uk --define SENTRY_RELEASE:\\'$SENTRY_RELEASE\\' --define SENTRY_ENVIRONMENT:\\'$SENTRY_ENVIRONMENT\\' \""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.10.12",
    "@sentry/nextjs": "^7.17.3",
    "jwt-decode": "^3.1.2",
    "nanoid": "^4.0.0",
    "next": "^13.0.1",
    "next-pwa": "^5.6.0",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "toucan-js": "^2.7.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.18.0",
    "@emotion/eslint-plugin": "^11.10.0",
    "@next/bundle-analyzer": "^13.0.1",
    "@next/eslint-plugin-next": "^13.0.0",
    "@sentry/cli": "^2.8.1",
    "@size-limit/preset-app": "^8.1.0",
    "@size-limit/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "concurrently": "^7.5.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ejs": "^0.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "imagemin": "^8.0.1",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "jest-junit": "^14.0.1",
    "progress-webpack-plugin": "^1.0.16",
    "typescript": "^4.8.4",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-bundle-tracker": "^1.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpack-sentry-plugin": "^2.0.3",
    "wrangler": "^2.1.13"
  }
}
