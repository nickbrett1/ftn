{
  "name": "main",
  "version": "1.0.0",
  "description": "Front-end for bemstudios.uk",
  "repository": {
    "type": "git",
    "url": "https://github.com/nickbrett1/bem",
    "directory": "webapp"
  },
  "size-limit": [
    {
      "path": "assets/webpack_bundles/main-*.js",
      "limit": "2.4 s"
    }
  ],
  "scripts": {
    "test": "jest --passWithNoTests",
    "build-dev": "npx webpack serve --env DEBUG --config webpack.dev.js",
    "analyze": "npx webpack --env ANALYZE DEBUG --config webpack.prod.js",
    "build": "webpack --config webpack.prod.js",
    "serve": "serve -n --ssl-cert ../localhost+2.pem --ssl-key ../localhost+2-key.pem assets/webpack_bundles/",
    "size": "npm run build && size-limit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@fontsource/roboto": "^4.5.8",
    "@mui/material": "^5.10.5",
    "@react-oauth/google": "^0.2.6",
    "@sentry/react": "^7.13.0",
    "jwt-decode": "^3.1.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.17.12",
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@emotion/eslint-plugin": "^11.10.0",
    "@sentry/cli": "^2.5.2",
    "@sentry/webpack-plugin": "^1.19.0",
    "@size-limit/preset-app": "^8.1.0",
    "@size-limit/webpack": "^8.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-plugin-transform-mui-imports": "^0.2.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-ejs": "^0.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.5.0",
