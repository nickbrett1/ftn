  "cSpell.enabledFileTypes": {
    "ignore": false,
    "json": false,
    "jsonc": false
  },
  "json.schemas": [
    {
      "fileMatch": ["/package.json"],
      "url": "https://json.schemastore.org/package.json"
    }
  ],
  "files.associations": {
    "*.svx": "markdown"
  }
}
