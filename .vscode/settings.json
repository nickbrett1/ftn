{
  "python.linting.pylintEnabled": true,
  "python.linting.enabled": true,
  "[jsx]": {
    "editor.tabSize": 2
  },
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "remote.localPortHost": "allInterfaces",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "task.allowAutomaticTasks": "on",
  "git.requireGitUserConfig": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.options": {
    "extensions": [".js", ".jsx", ".md", ".mdx", ".ts", ".tsx"]
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "svelte"
  ],
  "css.customData": [".vscode/tailwindcss.json"],
  "sonarlint.connectedMode.project": {
    "connectionId": "bem",
    "projectKey": "nickbrett1_bem"
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode",
    "javascript.validate.enable": false,
    "typescript.validate.enable": false
  },
  "liveServer.settings.port": 5502,
  "editor.fontSize": 16,
  "cSpell.words": [
    "agnoster",
    "Agnosticity",
    "Airbyte",
    "apexcharts",
    "AUTOINCREMENT",
    "BTCA",
    "btween",
    "buyside",
    "ccbilling",
    "ccbillingstatements",
    "chromedriver",
    "cimg",
    "conpago",
    "Dagster",
    "devcontainers",
    "distractedboyfriendmeme",
    "DOORDASH",
    "driftdb",
    "duckdb",
    "Fintech",
    "fintechnick",
    "Fivetran",
    "ggshield",
    "gitguardian",
    "gltf",
    "gltfjsx",
    "gsub",
    "Hyatt",
    "imagetools",
    "infostring",
    "Kiro",
    "Lakshmana",
    "lhci",
    "Linkify",
    "loglevel",
    "Matplotlib",
    "mdsvex",
    "miragejs",
    "mmdd",
    "nickbrett",
    "nojs",
    "noninteractive",
    "noopener",
    "NOSONAR",
    "notauthorised",
    "numpages",
    "oklch",
    "OLAP",
    "POWERLEVEL",
    "predev",
    "rclone",
    "RDBMS",
    "rgba",
    "robbyrussell",
    "rollup",
    "RPROMPT",
    "saos",
    "SCQA",
    "Seaborn",
    "shiki",
    "sonarcloud",
